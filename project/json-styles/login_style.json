{
  "QMainWindow": [
    {
      "tittle": "MealMatch",
      "icon": ":/font_awesome_brands/icons/font_awesome/brands/windows.png",
      "frameless": true,
      "transluscentBg": true,
      "navigation": [
        {
          "minimize": "minimize_window_button",
          "close": "close_window_button",
          "restore": [
            {
              "buttonName": "restore_window_button",
              "normalIcon": ":/icons/icons/maximize-2.svg",
              "maximizedIcon": ":/icons/icons/minimize-2.svg"
            }
          ],
          "moveWindow": "header_frame",
          "tittleBar": "header_frame"
        }
      ]
    }
  ],
  "LiveCompileQss": true,
  "ShowLogs": false,
  "CheckForMissingicons": false,
  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#f5f5f5",
              "Text-color": "#333333",
              "Accent-color": "#008080",
              "Icons-color": "#333333",
              "Theme-name": "Default-theme",
              "Default-Theme": true
            }
          ]
        }
      ],
      "AppSettings": {
        "OrginizationName": "MealMatch",
        "ApplicationName": "MealMatch Company",
        "OrginizationDormain": "MealMatch.org"
      }
    }
  ],
  "QCustomQStackedWidget": [
    {
      "name": "login_signup_stackedWidget",
      "navigation": [
        {
          "navigationButtons": [
            {
              "to_signup_button": "right_signup_page",
              "to_login_button": "right_login_page"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "slide": [
            {
              "active": true,
              "duration": 1000,
              "direction": "vertical",
              "easingCurve": "OutBack"
            }
          ]
        }
      ]
    },
    {
      "name": "welcome_stackedWidget",
      "navigation": [
        {
          "navigationButtons": [
            {
              "to_signup_button": "left_signup_page",
              "to_login_button": "left_login_page"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "slide": [
            {
              "active": true,
              "duration": 1000,
              "direction": "horizontal",
              "easingCurve": "OutBack"
            }
          ]
        }
      ]
    }
  ],
  "QtBinding": "PyQt6"
}